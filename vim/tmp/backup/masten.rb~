class Masten < ActiveRecord::Base
    attr_accessible :isMobiaStop, :stop,:room_id :location, :name, :operator, :street, :x, :y,:tags_attributes,:tags,:pillbox,:shield,:mastenFitout_attributes,:pillbox_attributes,:shield_attributes, :mastenFitout_id,:direction
    has_one :mastenFitout
    has_one :pillbox
    has_one :shield
    belongs_to :stop
    has_many :tags
    belongs_to :room
    accepts_nested_attributes_for :tags
    accepts_nested_attributes_for :mastenFitout
    accepts_nested_attributes_for :pillbox
    accepts_nested_attributes_for :shield

end
